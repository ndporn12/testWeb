<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" strict="true">
   <Appenders>
       <Console name="STDOUT">
           <PatternLayout pattern="%5p [%c{1}] - %m%n" />
       </Console>
   </Appenders>
   <Loggers>
       <Logger name="kr.ac.sunmoon.web"
               level="trace" additivity="false">
           <AppenderRef ref="STDOUT" />
       </Logger>
       <Root level="trace">
           <AppenderRef ref="STDOUT" />
       </Root>
   </Loggers>
</Configuration>